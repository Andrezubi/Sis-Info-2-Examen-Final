{
	"name": "df_transformar_warranties",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_bronze_tabla_warranty",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_warranty",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "cast1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          claim_id as string,",
				"          claim_date as string,",
				"          sale_id as string,",
				"          repair_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"derivedColumn1 cast(output(",
				"          claim_date as timestamp 'yyyy-MM-dd'",
				"     ),",
				"     errors: true) ~> cast1",
				"source1 derive(claim_date = case(\r",
				"    isNull(claim_date) || claim_date == '', '0000-00-00',\r",
				"    \r",
				"    instr(claim_date, '-')!=0,\r",
				"        claim_date,\r",
				"\r",
				"    instr(claim_date, '/')!=0,\r",
				"        toString(\r",
				"            toTimestamp(claim_date, 'dd/MM/yyyy'),\r",
				"            'yyyy-MM-dd'\r",
				"        ),\r",
				"\r",
				"    toString(\r",
				"        toTimestamp(claim_date, 'MMM dd yyyy'),\r",
				"        'yyyy-MM-dd'\r",
				"    )\r",
				")) ~> derivedColumn1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          claim_id as string,",
				"          claim_date as timestamp,",
				"          sale_id as string,",
				"          repair_status as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          claim_id,",
				"          claim_date,",
				"          sale_id,",
				"          repair_status",
				"     )) ~> sink1"
			]
		}
	}
}