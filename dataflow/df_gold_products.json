{
	"name": "df_gold_products",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_tabla_products",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dim_date",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_dim_product",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Product_ID as string,",
				"          Product_Name as string,",
				"          Category_ID as string,",
				"          Launch_Date as timestamp,",
				"          Price as integer,",
				"          notes as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(output(",
				"          dateId as integer,",
				"          fullDate as timestamp,",
				"          year as integer,",
				"          month as integer,",
				"          monthName as string,",
				"          day as integer,",
				"          quarter as integer,",
				"          weekdayName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1, source2 join(Launch_Date == fullDate,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 aggregate(groupBy(Product_ID),",
				"     each(match(name!='Product_ID'), $$ = first($$))) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Product_ID as string,",
				"          Product_Name as string,",
				"          Category_ID as string,",
				"          Launch_Date_id as integer,",
				"          Price as integer,",
				"          notes as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Product_ID,",
				"          Product_Name,",
				"          Category_ID,",
				"          Launch_Date_id = dateId,",
				"          Price,",
				"          notes",
				"     )) ~> sink1"
			]
		}
	}
}